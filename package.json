{
  "name": "windows-11-calculator",
  "version": "1.0.0",
  "description": "A web-based calculator replicating Windows 11 Calculator Basic Mode",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
